GET /hello.txt HTTP/1.1
Host: 127.0.0.1:8080
User-Agent: curl/8.0.1
Accept: */*
Connection: keep-alive

POST /json.txt HTTP/1.1
Host: 127.0.0.1:8080
User-Agent: curl/8.0.1
Content-Type: application/json
Content-Length: 20
Connection: keep-alive

{"key":"value"}

PUT /json.txt HTTP/1.1
Host: 127.0.0.1:8080
User-Agent: curl/8.0.1
