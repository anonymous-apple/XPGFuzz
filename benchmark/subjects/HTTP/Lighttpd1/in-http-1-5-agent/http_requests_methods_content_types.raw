Content-Length: 25
Connection: keep-alive

{"key":"value","num":123}

PATCH /json.txt HTTP/1.1
Host: 127.0.0.1:8080
User-Agent: curl/8.0.1
Content-Type: application/json
Content-Length: 20
Connection: keep-alive

{"op":"replace"}

POST /form.txt HTTP/1.1
Host: 127.0.0.1:8080
User-Agent: curl/8.0.1
Content-Type: application/x-www-form-urlencoded
Content-Length: 15
Connection: keep-alive

key1=value1

